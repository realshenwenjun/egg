<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dskj.comment.mapper.CommentImageMapper">
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache">
	</cache>
	<select id="clear" resultType="int" flushCache="true">
		SELECT 1
	</select>
	<insert id="add" parameterType="java.util.ArrayList">
		INSERT INTO comment_image
		(comment_id,img_path,create_time) VALUES
		<foreach collection="list" item="paths" index="index"
			separator=",">(#{paths.commentId},#{paths.imgPath},#{paths.createTime})
		</foreach>
	</insert>
</mapper>